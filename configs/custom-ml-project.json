        "branch_directories": {
            "main": "stable",
            "dev": "development",
            "experiments": "experiments"
        }
    },
    "directory_types": {
        "src": "ml_research",
        "tests": "tests",
        "docs": "documentation"
    },
    "ai_integration": "claude",
    "_comment": "Machine learning project with custom directories for data, models, and experiments. Note how private_dirs includes subdirectories like 'data/raw' for sensitive data."
